  }

  // helper functions for checking answers
  const ansSet = new Set(e.player_answers || []);
  const has = (id) => ansSet.has(id) || ansSet.has(String(id));
  const hasAny = (set) => {
    for (const id of set) if (has(id)) return true;
    return false;
  };

  // check if the menu already exists
  let popOutMenu = document.getElementById('popOutMenu');
  if (!popOutMenu) {
