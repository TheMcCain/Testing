    windowBody.className = 'window-body';
    windowBody.style.padding = '10px';
    windowBody.style.fontFamily = '"Segoe UI", Arial, sans-serif';

    // Top section
    const topSection = document.createElement('div');
    topSection.style.display = 'grid';
    topSection.style.gridTemplateColumns = '1.4fr 1fr';
    topSection.style.gap = '10px';
    topSection.style.marginBottom = '10px';

    // Administration Pane (fieldset + legend)
    const adminGroup = makeGroupBox('The Administration');
    const adminContent = document.createElement('ul');
    adminContent.style.listStyleType = 'disc';
    adminContent.style.listStylePosition = 'inside';
    adminContent.style.paddingLeft = '5px';
    adminContent.style.margin = '0';
    adminContent.style.fontSize = '11px';

    // helper function to add dynamic lines
    const addLine = (html) => {
      const li = document.createElement('li');
      li.innerHTML = html;
      adminContent.appendChild(li);
    };

    addLine(`${WinsMap[String(Wins)]}`);
    addLine(`${CredMap[String(Cred)]}`);
	
	if (Carrington > 0) {
        const instabilityText = InstabilityMap[String(Instability)] || `National instability: ${Instability}`;
        addLine(instabilityText);
    }

    if (hasAny(GARLAND_FAILED_IDS)) addLine(`We've <i>resorted to</i> judge <span style="color: #CE0F0F;">Merrick Garland</span>.`);
    if (hasAny(GARLAND_CHOSEN_IDS)) addLine(`We've <i>appointed</i> judge <span style="color: #FADC0E;">Merrick Garland</span>.`);
    if (hasAny(KAGAN_FAILED_IDS)) addLine(`We've <i>resorted to</i> judge <span style="color: #CE0F0F;">Elena Kagan</span>.`);
    if (hasAny(KAGAN_CHOSEN_IDS)) addLine(`We've <i>appointed</i> judge <span style="color: #FADC0E;">Elena Kagan</span>.`);
    if (Sullivan > 0) addLine(`We've <i>appointed</i> judge <span style="color: #FADC0E;">Kathleen Sullivan</span>.`);
    if (Napolitano > 0) addLine(`We've <i>appointed</i> judge <span style="color: #98D937;">Janet Napolitano</span>.`);
    if (Sotomayor > 0) addLine(`We've <i>appointed</i> judge <span style="color: #98D937;">Sonia Sotomayor</span>.`);
    if (Wood > 0) addLine(`We've <i>appointed</i> judge <span style="color: #98D937;">Diane Wood</span>.`);
    if (Sears > 0) addLine(`We've <i>appointed</i> judge <span style="color: #98D937;">Leah Ward Sears</span>.`);
    if (Harris > 0) addLine(`We've <i>appointed</i> judge <span style="color: #15A600;">Kamala Harris</span>.`);
    if (Holder > 0) addLine(`We've <i>appointed</i> judge <span style="color: #15A600;">Eric Holder</span>.`);
    if (Ellison > 0) addLine(`We've <i>appointed</i> judge <span style="color: #15A600;">Keith Ellison</span>.`);
    if (Liu > 0) addLine(`We've <i>appointed</i> judge <span style="color: #15A600;">Goodwin Liu</span>.`);
    if (hasAny(STIMULUS_1T1_IDS)) addLine(`We've <i>passed</i> a <span style="color: #15A600;">$1.1 trillion</span> stimulus plan.`);
    if (hasAny(STIMULUS_780B_IDS)) addLine(`We've <i>passed</i> a <span style="color: #98D937;">$780 billion</span> stimulus plan.`);
    if (has(8932)) addLine(`We've <i>passed</i> a <span style="color: #FADC0E;">$450 billion</span> stimulus plan.`);
    if (hasAny(HEALTH_AAHC_IDS)) addLine(`We've <i>signed</i> the <span style="color: #15A600;">America's Affordable</span><br><span style="color: #15A600;">Health Choices Act</span>.`);
    if (hasAny(HEALTH_PPAACA_IDS)) addLine(`We've <i>signed</i> the <span style="color: #98D937;">Patient Protection &</span><br><span style="color: #98D937;">Affordable Care Act</span>.`);
    if (hasAny(HEALTH_AHFA_IDS)) addLine(`We've <i>signed</i> the <span style="color: #FADC0E;">America's Healthy Future</span><br><span style="color: #FADC0E;">Act</span>.`);
    if (hasAny(HEALTH_FAIL_IDS)) addLine(`We've <em><span style="color: #CE0F0F;">failed healthcare reform</span></em>.`);
    if (hasAny(BK_IDS)) addLine(`We've <i>signed</i> the <span style="color: #15A600;">Brown-Kaufman Act</span>.`);
    if (hasAny(DODD_FRANK_IDS)) addLine(`We've <i>signed</i> the <span style="color: #98D937;">Dodd-Frank Act</span>.`);
    if (hasAny(DODD_SHELBY_IDS)) addLine(`We've <i>signed</i> the <span style="color: #FADC0E;">Dodd-Shelby Act</span>.`);
    if (has(NEPTUNE_SUCCESS_ID)) addLine(`Operation <strong>Neptune Spear</strong> is <span style="color: #98D937;">a success</span>.`);
    if (has(NEPTUNE_FAIL_ID)) addLine(`Operation <strong>Neptune Spear</strong> is <span style="color: #CE0F0F;">a failure</span>.`);

    adminGroup.appendChild(adminContent);

    // Latest News Pane (fieldset + legend)
    const newsGroup = makeGroupBox('Latest News');
    const qpic = document.createElement('img');
    qpic.src = pictureDict[e.noCounter] || "https://file.garden/aNtAfG887DiA_7lO/Obamanation/oFkHxtI.png";
    qpic.style.width = '135px';
    qpic.style.height = '152px';
    qpic.style.display = 'block';
    qpic.style.margin = '0 auto';
    qpic.style.border = '1px solid #808080';
    newsGroup.appendChild(qpic);

    topSection.appendChild(adminGroup);
    topSection.appendChild(newsGroup);
    windowBody.appendChild(topSection);

    // Opponent Index
    const opponentGroup = makeGroupBox('Opponent Index');
    const menuListA = document.createElement('ul');
    menuListA.className = 'opponents-list has-scrollbar';

    const fragment = document.createDocumentFragment();
    menuItemsA.forEach(item => {
      const menuItem = document.createElement('div');
      menuItem.className = 'candidate';
      if (FAKE_BG_IDS.has(item.id)) menuItem.classList.add('fake');

      // Photo
      const photo = document.createElement('img');
      photo.className = 'photo';
      photo.src = item.photo;
      menuItem.appendChild(photo);

      // Info column
      const info = document.createElement('div');
      info.className = 'info';

      // Name bar
      const namebar = document.createElement('div');
      namebar.className = 'namebar';

      const bullet = document.createElement('span');
      bullet.className = 'bullet ' + (GREEN_NAME_IDS.has(item.id) ? 'green' : 'blue');

      const name = document.createElement('span');
      name.className = 'cand-name';
      name.style.color = GREEN_NAME_IDS.has(item.id) ? "#15A600" : "#0045AC";
      name.textContent = item.name;

      namebar.appendChild(bullet);
      namebar.appendChild(name);
      info.appendChild(namebar);

      // Description
      const text = document.createElement('p');
      text.className = 'cand-text';
      text.innerHTML = item.text;
      info.appendChild(text);

      menuItem.appendChild(info);
      fragment.appendChild(menuItem);
    });

    menuListA.appendChild(fragment);
    opponentGroup.appendChild(menuListA);
    windowBody.appendChild(opponentGroup);
    popOutMenu.appendChild(windowBody);
    document.body.appendChild(popOutMenu);

    // apply draggable behavior to the title bar
    makeDraggable(popOutMenu, titleBar);
  }

  if (savedX !== null && savedY !== null) {
    popOutMenu.style.left = savedX + 'px';
    popOutMenu.style.top = savedY + 'px';
  } else {
    const evt = ev || window.event;
    const buttonRect = evt && evt.target && evt.target.getBoundingClientRect
      ? evt.target.getBoundingClientRect()
      : { right: window.innerWidth / 2, top: window.innerHeight / 2 };

    const initialX = buttonRect.right + window.scrollX + 10;
    const initialY = buttonRect.top + window.scrollY - 250;

    popOutMenu.style.left = initialX + 'px';
    popOutMenu.style.top = initialY + 'px';

    savedX = initialX;
    savedY = initialY;
  }

  popOutMenu.style.display = 'block';
}

function closeSurrogates() {
  const popOutMenu = document.getElementById('popOutMenu');
  if (popOutMenu) {
    savedX = popOutMenu.getBoundingClientRect().left;
    savedY = popOutMenu.getBoundingClientRect().top;
    popOutMenu.remove();
  }
}

function refreshSurrogates() {
  const popOutMenu = document.getElementById('popOutMenu');
  if (popOutMenu) {
    popOutMenu.remove();
    openSurrogates();
  }
}

function makeDraggable(element, handle) {
  const dragHandle = handle || element;
  let isDragging = false;
  let offsetX, offsetY;
  
  const updatePosition = (clientX, clientY) => {
    element.style.left = `${clientX - offsetX}px`;
    element.style.top = `${clientY - offsetY}px`;
    savedX = clientX - offsetX;
    savedY = clientY - offsetY;
  };
  
  const onMove = (e) => {
    if (!isDragging) return;
    const clientX = e.clientX ?? e.touches[0].clientX;
    const clientY = e.clientY ?? e.touches[0].clientY;
    updatePosition(clientX, clientY);
  };
  
  const onEnd = () => {
    if (!isDragging) return;
    isDragging = false;
    document.body.classList.remove('is-dragging');
    document.removeEventListener('mousemove', onMove);
    document.removeEventListener('mouseup', onEnd);
    document.removeEventListener('touchmove', onMove);
    document.removeEventListener('touchend', onEnd);
  };
  
  const onStart = (e) => {
    const target = e.target;
    if (['BUTTON', 'INPUT', 'SELECT', 'TEXTAREA', 'A'].includes(target.tagName)) {
      return;
    }
    
    isDragging = true;
    const rect = element.getBoundingClientRect();
    const clientX = e.clientX ?? e.touches[0].clientX;
    const clientY = e.clientY ?? e.touches[0].clientY;
    offsetX = clientX - rect.left;
    offsetY = clientY - rect.top;
    
    document.body.classList.add('is-dragging');
    document.addEventListener('mousemove', onMove);
    document.addEventListener('mouseup', onEnd);
    document.addEventListener('touchmove', onMove, { passive: false });
    document.addEventListener('touchend', onEnd);
    
    e.preventDefault();
  };
  
  dragHandle.addEventListener('mousedown', onStart);
  dragHandle.addEventListener('touchstart', onStart, { passive: false });
  
  return () => {
    dragHandle.removeEventListener('mousedown', onStart);
    dragHandle.removeEventListener('touchstart', onStart);
    onEnd();
  };
}

// prevent text highlighting while dragging
const dragStyle = document.createElement('style');
dragStyle.innerHTML = `.is-dragging * { user-select: none; }`;
document.head.appendChild(dragStyle);

e.initDC = false;

function addHeadquarterButton() {
  if (document.getElementById('headquarter_button')) {
    return;
  }

  const refButton = document.getElementById('view_electoral_map');
  if (!refButton) return;

  const newButton = document.createElement('button');
  newButton.id = 'headquarter_button';
  newButton.style.marginLeft = '1.5em';
  newButton.innerHTML = 'Deep State';

  newButton.addEventListener('click', openSurrogates);

  refButton.insertAdjacentElement('afterend', newButton);
}

async function handleFooter() {
  var gameWindow = document.getElementById('game_window');
  addHeadquarterButton();
};

async function handleMutations(mutationsList, observer) {
  if (observerRunning) return;
  observerRunning = true;
  observer.disconnect();
  await handleFooter();
  observer.observe(document.documentElement, { childList: true, subtree: true });
  observerRunning = false;
}
let processedNodes = new Set();
let observerRunning = false;

var element = document.getElementById('controlElement');
if (!element) {
  let singleObserver = new MutationObserver(handleMutations);
  singleObserver.observe(document.documentElement, { childList: true, subtree: true });
  var controlElement = document.createElement('div');
  controlElement.style.display = 'none';
  controlElement.id = 'controlElement';
  document.body.appendChild(controlElement);
}

const gameWindowNode = document.getElementById("game_window");
const candidateWatcherConfig = { attributes: true, childList: true, subtree: true };

const candidateObserver = new MutationObserver(onGameWindowChangeCandidates);
if (gameWindowNode) {
  candidateObserver.observe(gameWindowNode, candidateWatcherConfig);
}

